<section class="main-content">
  <div class="container">
    <h1>Ranking de Alumnos</h1>
    <br>
    <br>
    <div class="row">
      <div class="col-sm-4">
        <div class="leaderboard-card">
          <div class="leaderboard-card__top">
            <h3 class="text-center">Tercero Puesto</h3>
          </div>
          <div class="leaderboard-card__body">
            <div class="text-center">
              <img src="https://cdn-icons-png.flaticon.com/512/7645/7645366.png" class="circle-img mb-2" alt="User Img">
              <h4 class="mb-0">{{TercerPuesto.name}}</h4>
              <p class="text-muted mb-0">{{TercerPuesto.email}}</p>
              <hr>
              <div class="d-flex justify-content-between align-items-center">

              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="leaderboard-card leaderboard-card--first">
          <div class="leaderboard-card__top">
            <h3 class="text-center">Primer Puesto</h3>
          </div>
          <div class="leaderboard-card__body">
            <div class="text-center">
              <img src="https://cdn-icons-png.flaticon.com/512/7645/7645279.png" class="circle-img mb-2" alt="User Img">
              <h4 class="mb-0">{{PrimerPuesto.name}}</h4>
              <p class="text-muted mb-0 ">{{PrimerPuesto.email}}</p>
              <hr>
              <div class="d-flex justify-content-between align-items-center">
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="leaderboard-card">
          <div class="leaderboard-card__top">
            <h3 class="text-center">Segundo Puesto</h3>
          </div>
          <div class="leaderboard-card__body">
            <div class="text-center">
              <img src="https://cdn-icons-png.flaticon.com/512/7645/7645294.png" class="circle-img mb-2" alt="User Img">

              <h4 class="mb-0">{{SegundoPuesto.name}}</h4>
              <p class="text-muted mb-0">{{SegundoPuesto.email}}</p>
              <hr>
              <div class="d-flex justify-content-between align-items-center">

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


    <h4></h4>

    <table class="table">
      <thead>
        <tr>
          <th>Posicion</th>
          <th>Nombre</th>
          <th>Apellido</th>
          <th>Puntaje</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        {{#each ranking}}
        <tr>
          <td>
            <div class="d-flex align-items-center">
              {{!-- <img src="img/user1.jpg" class="circle-img circle-img--small mr-2" alt="User Img"> --}}
              <div class="user-info__basic">
                <h4 class="mb-0">{{id}}</h4>
              </div>
            </div>

          </td>
          <td>
            <div class="d-flex align-items-center">
              {{!-- <img src="img/user1.jpg" class="circle-img circle-img--small mr-2" alt="User Img"> --}}
              <div class="user-info__basic">

                <h4 class="mb-0">{{name}}</h4>

              </div>
            </div>
          </td>
          <td>
            <div class="d-flex align-items-center">
              {{!-- <img src="img/user1.jpg" class="circle-img circle-img--small mr-2" alt="User Img"> --}}
              <div class="user-info__basic">

                <h4 class="mb-0">{{apellido}}</h4>

              </div>
            </div>
          </td>
          <td>
            <div class="d-flex align-items-baseline">
              <h4 class="mr-1">{{puntaje}}</h4><small class="text-success"></small>
            </div>
          </td>

          <td>{{email}}</td>

        </tr>
        {{/each}}


      </tbody>
    </table>
  </div>
</section>